  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "prepare": "husky",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "clean": "rm -rf node_modules",
    "test": "vitest",
    "update-commit": "bash scripts/update_commit.sh"
